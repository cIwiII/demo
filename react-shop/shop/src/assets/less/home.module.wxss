.home {
  max-width: 150rem;
  width: 100%;
  min-width: max-content;
  min-height: max-content;
  height: 100vh;
  margin: 0 auto;
}
.home .header {
  height: 8rem;
  padding: 0 2.5rem;
  display: flex;
  justify-content: space-between;
}
.home .header img {
  width: 15rem;
  height: 4rem;
  margin: 3rem 0 0 0rem;
}
.home .header .tab {
  display: flex;
  width: 90rem;
  height: 4rem;
  margin-top: 4rem;
  align-items: center;
}
.home .header .tab div {
  line-height: 3rem;
  background-color: rgba(100, 231, 231, 0.3);
  padding: 0 1rem;
  padding-left: 2rem;
  margin: 0 1px;
  border: 1px solid #9b9696;
  border-radius: 5px;
}
.home .header .tab div a {
  text-decoration: none;
}
.home .header .tab div button {
  margin-left: 1rem;
  font-size: 2rem;
  background-color: transparent;
  border: none;
  cursor: pointer;
}
.home .header .tab .active {
  background-color: #31d608;
}
.home .header .logHead {
  margin: 1rem 5rem 0 0;
}
.home .header .logHead span {
  display: inline-block;
  margin-right: 2rem;
  color: #fff;
}
.home .main {
  display: flex;
  justify-content: space-between;
}
.home .main ul {
  margin-top: 2rem;
  list-style: none;
  text-align: center;
  color: #fff;
}
.home .main ul li {
  line-height: 4rem;
}
.home .main ul li .a {
  text-decoration: none;
}
.home .main ul .active {
  background-color: #4ed630;
}
.home .main .contents {
  padding: 1rem 2rem;
  background-color: #f3ebeb;
}
.home .main .contents .content {
  background-color: #fff;
  height: 100%;
}
.active {
  background-color: greenyellow;
}
